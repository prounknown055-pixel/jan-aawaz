{
  "expo": {
    "name": "Jan Aawaz",
    "slug": "jan-aawaz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.janaawaz.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "VIBRATE"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.501851319502-g7okcl7nui3bhpqn72egu9r03uii36l3"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Jan Aawaz ko aapki location chahiye map par problems dikhane ke liye."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#FF6B35"
        }
      ]
    ],
    "scheme": "janaawaz",
    "extra": {
      "eas": {
        "projectId": "e283492a-8642-4c3c-93c6-6686c9bf8c67"
      },
      "router": {
        "origin": false
      }
    },
    "sdkVersion": "51.0.0",
    "platforms": ["ios", "android"]
  }
}
